function customgearClick()

local x = game:GetService("InsertService"):LoadAsset(interface.settings.Pge1["Custom Gear ID"].TextBox.Text)
for i,v in pairs(x:GetChildren()) do
v.Parent = game.Players.LocalPlayer.Backpack
end
x:Remove()
end

customgear.MouseButton1Down:connect(customgearClick)

