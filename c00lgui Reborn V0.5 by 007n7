function leftarrowClick()

if Page1.Visible == true then
Page1.Visible = false
Page2.Visible = true
elseif Page2.Visible == true then
Page2.Visible = false
Page3.Visible = true
elseif Page3.Visible == true then
Page3.Visible = false
Page4.Visible = true
elseif Page4.Visible == true then
Page4.Visible = false
Page5.Visible = true
elseif Page5.Visible == true then
Page5.Visible = false
Page1.Visible = true
end
end

leftarrow.MouseButton1Down:connect(leftarrowClick)

rightarrow = Instance.new("TextButton")
rightarrow.Name = (">")
rightarrow.Parent = interface
rightarrow.BackgroundColor3 = Color3.new(0/255, 0/255, 0/255)
rightarrow.BorderColor3 = Color3.new(255/255, 0/255, 0/255)
rightarrow.BorderSizePixel = 3
rightarrow.Position = UDim2.new(0.5, 3, 0, 40)
rightarrow.Size = UDim2.new(0.5, -3, 0, 40)
rightarrow.ZIndex = 2
rightarrow.Font = ("SourceSans")
rightarrow.FontSize = ("Size48")
rightarrow.Text = (">")
rightarrow.TextColor3 = Color3.new(255/255, 255/255, 255/255)

function rightarrowClick()

if Page5.Visible == true then
Page5.Visible = false
Page4.Visible = true
elseif Page4.Visible == true then
Page4.Visible = false
Page3.Visible = true
elseif Page3.Visible == true then
Page3.Visible = false
Page2.Visible = true
elseif Page2.Visible == true then
Page2.Visible = false
Page1.Visible = true
elseif Page1_2.Visible == true then
Page1.Visible = false
Page5.Visible = true
end
end

rightarrow.MouseButton1Down:connect(rightarrowClick)

