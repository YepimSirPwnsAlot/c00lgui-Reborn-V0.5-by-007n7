save.Size = UDim2.new(0, 150, 0, 20)
save.ZIndex = 2
save.Selected = true
save.Font  = ("SourceSans")
save.FontSize = ("Size18")
save.Text = ("Save IDs")
save.TextColor3 = Color3.new(255/255, 255/255, 255/255)

function saveClick()
    local skyboxInput = interface.settings.Pge1["Skybox/Decal ID"].TextBox.Text
    local musicInput = interface.settings.Pge1["Music ID"].TextBox.Text

    if skyboxInput ~= "" then
        lastSavedSkyboxID = skyboxInput
    end

    if musicInput ~= "" then
        lastSavedMusicID = musicInput
    end
end

save.MouseButton1Down:connect(saveClick)

load = Instance.new("TextButton")
load.Name = ("Load Button")
load.Parent = settings
load.BackgroundColor3 = Color3.new(0/255, 0/255, 0/255)
load.BorderColor3 = Color3.new(255/255, 0/255, 0/255)
load.BorderSizePixel = 3
load.Position = UDim2.new(0.5, 0, 0.300000012, 260)
load.Size = UDim2.new(0, 150, 0, 20)
load.ZIndex = 2
load.Selected = true
load.Font = ("SourceSans")
load.FontSize = ("Size18")
load.Text = ("Load IDs")
load.TextColor3 = Color3.new(255/255, 255/255, 255/255)

function loadClick()
    if lastSavedSkyboxID ~= nil then
        interface.settings.Pge1["Skybox/Decal ID"].TextBox.Text = lastSavedSkyboxID
    else
        interface.settings.Pge1["Skybox/Decal ID"].TextBox.Text = "158118263"
    end

    if lastSavedMusicID ~= nil then
        interface.settings.Pge1["Music ID"].TextBox.Text = lastSavedMusicID
    else
        interface.settings.Pge1["Skybox/Decal ID"].TextBox.Text = "157772998"
    end
end

load.MouseButton1Down:connect(loadClick)

