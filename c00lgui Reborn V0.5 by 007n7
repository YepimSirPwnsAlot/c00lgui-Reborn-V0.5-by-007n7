closeopen = Instance.new("TextButton")
closeopen.Name = ("Close/Open")
closeopen.Parent = servertrollgui
closeopen.BackgroundColor3 = Color3.new(0/255, 0/255, 0/255)
closeopen.BorderColor3 = Color3.new(255/255, 0/255, 0/255)
closeopen.BorderSizePixel = 3
closeopen.Position = UDim2.new(0,3,0.3,380)
closeopen.Size = UDim2.new(0,300,0,20)
closeopen.ZIndex = 3
closeopen.Font = ("SourceSans")
closeopen.FontSize = ("Size18")
closeopen.Text = ("Close")
closeopen.TextColor3 = Color3.new(255/255, 255/255, 255/255)

function closeopenClick()
if closeopen.Text == "Close" then
interface.Visible = false	
closeopen.Text = "Open"
else
interface.Visible = true
closeopen.Text = "Close"
end
end

closeopen.MouseButton1Down:connect(closeopenClick)

loadmsg = Instance.new("Message")
loadmsg.Parent = game.Workspace
loadmsg.Text = ("c00lgui Reborn V"..Version.." has loaded.")
wait(3)
loadmsg:Remove()
