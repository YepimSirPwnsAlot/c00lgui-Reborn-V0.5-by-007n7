-- Properties

funiservertrollgui.Name = "funiservertrollgui"
funiservertrollgui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
funiservertrollgui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
funiservertrollgui.ResetOnSpawn = true

Frame.Parent = funiservertrollgui
Frame.BackgroundColor3 = Color3.new(0, 0, 0)
Frame.BorderColor3 = Color3.new(1, 0, 0)
Frame.BorderSizePixel = 3
Frame.Position = UDim2.new(0, 3, 0.300000012, 0)
Frame.Size = UDim2.new(0, 300, 0, 400)
Frame.ZIndex = 2

Title.Name = "Title"
Title.Parent = Frame
Title.BackgroundColor3 = Color3.new(0, 0, 0)
Title.BorderColor3 = Color3.new(1, 0, 0)
Title.BorderSizePixel = 3
Title.Size = UDim2.new(1, 0, 0, 40)
Title.ZIndex = 2
Title.Font = Enum.Font.SourceSans
Title.Text = "c00lgui Reborn V0.5 by 007n7"
Title.TextColor3 = Color3.new(1, 1, 1)
Title.TextSize = 24

right.Name = "RightButton"
right.Parent = Frame
right.BackgroundColor3 = Color3.new(0, 0, 0)
right.BorderColor3 = Color3.new(1, 0, 0)
right.BorderSizePixel = 3
right.Position = UDim2.new(0.5, 3, 0, 40)
right.Size = UDim2.new(0.5, -3, 0, 40)
right.ZIndex = 2
right.Font = Enum.Font.SourceSans
right.FontSize = Enum.FontSize.Size48
right.Text = ">"
right.TextColor3 = Color3.new(1, 1, 1)
right.TextSize = 48
right.MouseButton1Click:Connect(function()
if Page1_2.Visible == true then
		Page1_2.Visible = false
		Page5.Visible = true
	elseif Page2_2.Visible == true then
		Page2_2.Visible = false
		Page1_2.Visible = true
	elseif Page3.Visible == true then
		Page3.Visible = false
		Page2_2.Visible = true
	elseif Page4.Visible == true then
		Page4.Visible = false
		Page3.Visible = true
	elseif Page5.Visible == true then
		Page5.Visible = false
		Page4.Visible = true
	end	
end)

left.Name = "LeftButton"
left.Parent = Frame
left.BackgroundColor3 = Color3.new(0, 0, 0)
left.BorderColor3 = Color3.new(1, 0, 0)
left.BorderSizePixel = 3
left.Position = UDim2.new(0, 0, 0, 40)
left.Size = UDim2.new(0.5, 0, 0, 40)
left.ZIndex = 2
left.Font = Enum.Font.SourceSans
left.FontSize = Enum.FontSize.Size48
left.Text = "<"
left.TextColor3 = Color3.new(1, 1, 1)
left.TextSize = 48
left.MouseButton1Click:Connect(function()
if Page1_2.Visible == true then
		Page1_2.Visible = false
		Page5.Visible = true
	elseif Page2_2.Visible == true then
		Page2_2.Visible = false
		Page1_2.Visible = true
	elseif Page3.Visible == true then
		Page3.Visible = false
		Page2_2.Visible = true
	elseif Page4.Visible == true then
		Page4.Visible = false
		Page3.Visible = true
	elseif Page5.Visible == true then
		Page5.Visible = false
		Page4.Visible = true
	end	
end)

Settings.Name = "Settings"
Settings.Parent = Frame
Settings.BackgroundColor3 = Color3.new(0, 0, 0)
Settings.BorderColor3 = Color3.new(1, 0, 0)
Settings.BorderSizePixel = 3
Settings.Position = UDim2.new(1, 3, 0, 0)
Settings.Size = UDim2.new(1, 0, 1, 0)

SettingsButton.Name = "SettingsButton"
SettingsButton.Parent = Settings
SettingsButton.BackgroundColor3 = Color3.new(0, 0, 0)
SettingsButton.BorderColor3 = Color3.new(1, 0, 0)
SettingsButton.BorderSizePixel = 3
SettingsButton.Position = UDim2.new(1, 3, 0, 0)
SettingsButton.Size = UDim2.new(0, 27, 1, 0)
SettingsButton.Font = Enum.Font.SourceSans
SettingsButton.FontSize = Enum.FontSize.Size48
SettingsButton.Text = "<"
SettingsButton.TextColor3 = Color3.new(1, 1, 1)
SettingsButton.TextSize = 48
local cango = true

SettingsButton.MouseButton1Click:Connect(function()
	if cango == true then
		cango = false
		if SettingsButton.Text == "<" then
			SettingsButton.Text = ">"
			repeat
				wait()
				Settings.Position = UDim2.new(1, Settings.Position.X.Offset - 10, 0, 0)
			until Settings.Position.X.Offset <= -293
			wait()
			Settings.Position = UDim2.new(1, -300, 0, 0)
		else
			SettingsButton.Text = "<"
			repeat
				wait()
				Settings.Position = UDim2.new(1, Settings.Position.X.Offset + 10, 0, 0)
			until Settings.Position.X.Offset >= -10
			wait()
			Settings.Position = UDim2.new(1, 3, 0, 0)
		end
		cango = true
	end
end)
