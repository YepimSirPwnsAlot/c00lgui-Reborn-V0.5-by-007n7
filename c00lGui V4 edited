MiddleCol = "Brown"
ViiniCol = "Brown"
Icon = "http://www.roblox.com/asset/?id=51902588"
Keys = {
e = false,
}
ModelName = "Epic Bow"
CA = CFrame.Angles
CN = CFrame.new
MR = math.rad
MP = math.pi
MRA = math.random
MH = math.huge
UD = UDim2.new
C3 = Color3.new
MaximumPower = 1000000000
MaxSpecial = 100000
Special = MaxSpecial
Sounds = {
Slash = {"rbxasset://sounds//swordslash.wav", 1.2, 1},
Shoot = {"http://www.roblox.com/asset/?id=16211041", 2, 1},
