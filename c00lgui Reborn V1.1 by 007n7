local c00lgui = Instance.new("ScreenGui")
c00lgui.Name = ("c00lgui")
c00lgui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
c00lgui.ResetOnSpawn = false

interface = Instance.new("Frame", c00lgui)
interface.BackgroundColor3 = Color3.new(0/255, 0/255, 0/255)
interface.BorderColor3 = Color3.new(255/255, 0/255, 0/255)
interface.BorderSizePixel = 3
interface.Position = UDim2.new(0, 3, 0.300000012, 0)
interface.Size = UDim2.new(0, 300, 0, 400)
interface.ZIndex = 2

scriptname = Instance.new("TextLabel", interface)
scriptname.Name = ("Script Name")
scriptname.BackgroundColor3 = Color3.new(0/255, 0/255, 0/255)
scriptname.BorderColor3 = Color3.new(255/255, 0/255, 0/255)
scriptname.BorderSizePixel = 3
scriptname.Size = UDim2.new(1, 0, 0, 40)
scriptname.ZIndex = 2
scriptname.Font = ("SourceSans")
scriptname.FontSize = ("Size24")
scriptname.Text = ("c00lgui Reborn V0.5 by 007n7")
scriptname.TextColor3 = Color3.new(255/255, 255/255, 255/255)

left = Instance.new("TextButton", interface)
left.Name = ("<")
left.BackgroundColor3 = Color3.new(0/255, 0/255, 0/255)
left.BorderColor3 = Color3.new(255/255, 0/255, 0/255)
left.BorderSizePixel = 3
left.Position = UDim2.new(0, 0, 0, 40)
left.Size = UDim2.new(0.5, 0, 0, 40)
left.ZIndex = 2
left.Font = ("SourceSans")
left.FontSize = ("Size48")
left.Text = ("<")
left.TextColor3 = Color3.new(255/255, 255/255, 255/255)

function left()

if Page1_2.Visible == true then
Page1_2.Visible = false
Page2_2.Visible = true
elseif Page2_2.Visible == true then
Page2_2.Visible = false
Page3.Visible = true
elseif Page3.Visible == true then
Page3.Visible = false
Page4.Visible = true
elseif Page4.Visible == true then
Page4.Visible = false
Page5.Visible = true
elseif Page5.Visible == true then
Page5.Visible = false
Page1_2.Visible = true
end
end

left.MouseButton1Down:connect(leftClick)

right = Instance.new("TextButton", interface)
right.Name = (">")
right.BackgroundColor3 = Color3.new(0/255, 0/255, 0/255)
right.BorderColor3 = Color3.new(255/255, 0/255, 0/255)
right.BorderSizePixel = 3
right.Position = UDim2.new(0.5, 3, 0, 40)
right.Size = UDim2.new(0.5, -3, 0, 40)
right.ZIndex = 2
right.Font = ("SourceSans")
right.FontSize = ("Size48")
right.Text = (">")
right.TextColor3 = Color3.new(255/255, 255/255, 255/255)

function right()

if Page5.Visible == true then
Page5.Visible = false
Page4.Visible = true
elseif Page4.Visible == true then
Page4.Visible = false
Page3.Visible = true
elseif Page3.Visible == true then
Page3.Visible = false
Page2_2.Visible = true
elseif Page2_2.Visible == true then
Page2_2.Visible = false
Page1_2.Visible = true
elseif Page1_2.Visible == true then
Page1_2.Visible = false
Page5.Visible = true
end
end

right.MouseButton1Down:connect(rightClick)

settings = Instance.new("Frame", interface)
settings.Name = ("Settings Tab")
settings.BackgroundColor3 = Color3.new(0/255, 0/255, 0/255)
settings.BorderColor3 = Color3.new(255/255, 0/255, 0/255)
settings.BorderSizePixel = 3
settings.Position = UDim2.new(1, 3, 0, 0)
settings.Size = UDim2.new(1, 0, 1, 0)

SettingsButton = Instance.new("TextButton", settings)
settingsbutton.Name = ("Settings Button")
settingsbutton.BackgroundColor3 = Color3.new(0, 0, 0)
settingsbutton.BorderColor3 = Color3.new(1, 0, 0)
settingsbutton.BorderSizePixel = 3
settingsbutton.Position = UDim2.new(1, 3, 0, 0)
settingsbutton.Size = UDim2.new(0, 27, 1, 0)
settingsbutton.Font = ("SourceSans")
SettingsButton.FontSize = ("Size48")
settingsbutton.Text = ("<")
settingsbutton.TextColor3 = Color3.new(255/255, 255/255, 255/255)

local cango = true

function settingsbutton()

	if cango == true then
		cango = false
		if SettingsButton.Text == "<" then
			SettingsButton.Text = ">"
			repeat
				wait()
				Settings.Position = UDim2.new(1, Settings.Position.X.Offset - 10, 0, 0)
			until Settings.Position.X.Offset <= -293
			wait()
			Settings.Position = UDim2.new(1, -300, 0, 0)
		else
			SettingsButton.Text = "<"
			repeat
				wait()
				Settings.Position = UDim2.new(1, Settings.Position.X.Offset + 10, 0, 0)
			until Settings.Position.X.Offset >= -10
			wait()
			Settings.Position = UDim2.new(1, 3, 0, 0)
		end
		cango = true
	end
end)

SettingsButton.MouseButton1Down:connect(settingsbuttonClick)
